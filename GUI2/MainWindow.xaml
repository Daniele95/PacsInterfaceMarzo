<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="PacsInterface" Height="450" Width="800">
    <Grid>
        <Frame x:Name="frame" x:FieldModifier="public" Content="" HorizontalAlignment="Left" Height="398" Margin="0,25,0,0" VerticalAlignment="Top" Width="792"/>
        <DockPanel HorizontalAlignment="Left" Height="26" LastChildFill="False" VerticalAlignment="Top" Width="792">
            <Button x:Name="queryPageButton" Content="Query Server" Height="26" VerticalAlignment="Top" Width="75" Click="queryPageButton_Click"/>
            <Button x:Name="downloadPageButton" Content="Download" Width="75" Click="downloadPageButton_Click"/>
            <Button x:Name="localPageButton" Content="Local Studies" Width="75" Click="localPageButton_Click" Height="26" VerticalAlignment="Bottom"/>
            <Button x:Name="localSeriesButton" Content="Local Series" Width="75" Click="localSeriesButton_Click" Height="26" VerticalAlignment="Top"/>
        </DockPanel>

    </Grid>
</Window>
